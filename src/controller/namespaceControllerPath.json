{
  "name":"框架应用",
  "author": "pizepei",
  "baseAuthGroup": {
    "staging": {
      "name":"应用项目",
      "explain": ""
    },
    "document":{
      "name":"项目文档",
      "explain": ""
    }
  },
  "constraint": {
    "required":{"type": "class","value": "required","dataType": "","msg":{"zh-cn": "必须的"}},
    "enum":{"type": "class","value": "enum","dataType": "string","msg":{"zh-cn": "只能是:{{0}},当前{{1}}"}},
    "money":{"type": "class","value": "moneyFormat","dataType": "string","msg":{"zh-cn": "货币格式:{{0}}"}},
    "mobile":{"type": "regexp","value": "/^[1][3-9][0-9]{9}$/","dataType": "int","msg":{"zh-cn": "手机号码格式错误"}},
    "identity":{"type": "regexp","value": "/^[1-9]\\d{5}(18|19|2([0-9]))\\d{2}(0[0-9]|10|11|12)([0-2][1-9]|30|31)\\d{3}[0-9X]$/","dataType": "string","msg":{"zh-cn": "身份证格式错误"}},
    "phone":{"type": "regexp","value": "/^(13[0-9]|14[5-9]|15[0-3,5-9]|16[2,5,6,7]|17[0-8]|18[0-9]|19[1,3,5,8,9])\\d{8}$/","dataType": "string","msg":{"zh-cn": "电话号码格式错误"}},
    "password":{"type": "regexp","value": "/^[0-9A-Za-z_\\@\\*\\,\\.]{6,23}$/","dataType": "string","msg":{"zh-cn": "密码格式错误"}},
    "uuid":{"type": "regexp","value": "/^[0-9A-Za-z]{8}[-][0-9A-Za-z]{4}[-][0-9A-Za-z]{4}[-][0-9A-Za-z]{4}[-][0-9A-Za-z]{12}$/","dataType": "string","msg":{"zh-cn": "uuid格式错误"}},
    "mid":{"type": "regexp","value": "/^(?![0-9]+$)[0-9A-Za-z_]{3,15}$/","dataType": "string","msg":{"zh-cn": "账户ID格式错误"}},
    "email":{"type": "regexp","value": "/^[A-Za-z\\d]+([-_.][A-Za-z\\d]+)*@([A-Za-z\\d]+[-.])+[A-Za-z\\d]{2,4}$/","dataType": "string","msg":{"zh-cn": "email格式错误"}}
  }
}
